{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "[PowerShell] Run Current File",
            "script": "${file}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "[Node] Run Current File",
            "program": "${file}",
            "args": [
                "./data/ca_staff_eds.json",
                "./data/Staff_JSON_Paths.json"
            ]
        }
    ]
}
